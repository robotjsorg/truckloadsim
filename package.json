{
  "name": "truckloadsim",
  "version": "0.0.0-development",
  "description": "Robotic simulation of loading and unloading a truck.",
  "repository": {
    "url": "https:git@github.com:jmcmahon443/truckloadsim.git",
    "type": "git"
  },
  "scripts": {
    "lint": "eslint --ext .jsx,.js src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "precommit": "lint-staged && yarn lint",
    "prettier": "prettier --single-quote --write \"src/**/*.{js,jsx}\""
  },
  "contributors": [
    {
      "name": "Joe McMahon",
      "email": "jmcmahon443@gmail.com"
    }
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.3.0"
  },
  "homepage": "https://jmcmahon443.github.io/truckloadsim/",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "debug": "^4.2.0",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.22.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "last-release-git": "^0.0.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.19.1",
    "react-scripts": "^4.0.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "aframe": "^1.2.0",
    "aframe-react": "^4.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
